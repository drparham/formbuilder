{
	"name": "pta/formbuilder",
	"type": "platform-extension",
	"description": "Automatically generate form definitions from SQL Schema",
	"authors": [
		{
			"name": "Dustin Parham",
			"email": "dr.parham@gmail.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.anointedbrotherhood.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.2.*",
		"illuminate/support": "4.2.*",
		"platform/foundation": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Abh\\Formbuilder\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
